.theme-default-content pre, .theme-default-content pre[class*=language-] {
    line-height: 1.4;
    padding: 1.25rem 1.5rem;
    margin: 0;
    background-color: #282c34;
    border-radius: 6px;
    overflow: auto;
}
div[class*="language-"] pre, div[class*="language-"] pre[class*="language-"] {
    background: transparent;
    position: relative;
    z-index: 1;
    border: #282c34;
    border-radius: 0px;
}
